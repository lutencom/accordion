{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/accordion",
  "version": "0.1.0",
  "title": "Accordion",
  "category": "widgets",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "attributes": {
    "border": {
      "type": "object",
      "default": {
        "width": "1px",
        "color": "#EAE9ED",
        "style": "solid"
      }
    },
    "borderRadius": {
      "type": "number",
      "default": 10
    }
  },
  "supports": {
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "color": {
      "background": true,
      "text": false
    },
    "ariaLabel": true,
    "align": [
      "left",
      "right",
      "wide",
      "full"
    ]
  },
  "textdomain": "accordion",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}